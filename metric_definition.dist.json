[
  {
    "service_name": "grafana",
    "url": "http://grafana",
    "request_headers": [],
    "metrics": [
      {
        "metric_name": "database_health",
        "api_endpoint": "/api/health",
        "desired_response_field": "database",
        "string_value_mapping": {
          "ok": 1.0,
          "not ok": 0.0
        }
      },
      {
        "metric_name": "custom_output",
        "program": "python",
        "argument": "custom_script.py"
      }
    ]
  }
]